{
  "name": "gonature",
  "version": "2.0.0",
  "description": "vacation package booking app with nodejs, mongoDB, express, React, Redux",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "start:dev": "SET NODE_ENV=development&nodemon server.js",
    "start:prod": "SET NODE_ENV=production&nodemon server.js",
    "dev": "concurrently \"npm run start:dev\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Juyean Lee",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "concurrently": "^5.3.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.0.2",
    "express-rate-limit": "^5.2.3",
    "express-session": "^1.17.1",
    "helmet": "^4.4.1",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.12",
    "morgan": "^1.10.0",
    "node-sass": "^5.0.0",
    "nodemailer": "^6.4.17",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "slugify": "^1.4.6",
    "stripe": "^8.135.0",
    "validator": "^13.5.2",
    "xss-clean": "^0.1.1"
  }
}
